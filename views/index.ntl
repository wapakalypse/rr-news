    <script>

        let buttonsMore = document.querySelectorAll(".content-news-more");

        [].forEach.call( buttonsMore, function(el) {
            el.onclick = function(e) {
                let number = Number(el.id);
                let newsNumber = document.querySelector('#news' + number);
                let newsElementsP = newsNumber.getElementsByTagName('p');

                let elementP = null;

                for (let i = 0; i < newsElementsP.length; ++i) {
                    elementP = newsElementsP[i];
                    elementP.classList.add('show-p');
                }

            el.classList.add('fade');
            }
        });

